    "supertest": "^4.0.2",
    "testcafe": "^1.8.4",
    "testcafe-react-selectors": "^4.0.0",
    "uuid": "^3.4.0",
    "wait-on": "^3.3.0",
    "webdriverio": "^5.22.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voluntarily/vly2.git"
  },
  "author": "Andrew Watkins",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/voluntarily/vly2/issues"
  },
  "homepage": "https://github.com/voluntarily/vly2#readme",
  "ava": {
    "files": [
      "__tests__/**/*.spec.js",
      "server/**/__tests__/*.spec.js",
      "components/**/__tests__/*.spec.js",
      "lib/**/__tests__/*.spec.js"
    ],
    "helpers": [
      "**/__tests__/**/*.fixture.js",
      "lib/react-intl-test*.js"
    ],
    "sources": [
      "components/**/*.js",
      "hocs/**/*.js",
      "lib/**/*.js",
      "pages/**/*.js",
      "server/**/*.js"
    ],
    "failFast": false,
    "timeout": "2m",
    "babel": {
      "testOptions": {
        "retainLines": true,
        "plugins": [
          "@babel/plugin-syntax-jsx"
        ],
        "presets": [
          "next/babel"
        ]
      }
    },
    "require": [
      "./server/util/setup-test-env.js"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "ignore-class-method": "GetInitialProps",
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    },
    "include": [
      "pages/**/*.js",
      "pages/api/*.js",
      "components/**/*.js",
      "server/**/*.js",
      "lib/**/*.js"
    ],
    "exclude": [
      "**/__tests__/**",
      "**/*.stories.js",
      "**/*.spec.js",
      "**/*.fixture.js",
      "**/*.init.js",
      "**/__init__/*.js",
      "server/util/*.js",
      "server/util/__tests__/*.js",
      "lib/react-intl-test*.js",
      "lib/auth/auth0.js",
      "server/config.js",
      "components/examples/**/*.js",
      "**/*-md.js",
      "pages/_*.js",
      "pages/test/**/*.js",
      "pages/todo.js",
      "pages/admin/admin.js",
      "pages/admin/goals/*.js",
      "pages/teachers/*.js",
      "pages/business/*.js",
      "pages/content/*.js",
      "server/api/image/cloudImageUpload.js",
      "server/services/email/email.js",
      "reference",
      "systemtest",
      "x",
      "public"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "standard": {
    "plugins": [
      "html"
    ],
    "parser": "babel-eslint"
  }
}
