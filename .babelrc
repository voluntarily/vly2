{
  "presets": ["next/babel"],
  "env": {
    "development": {
      "plugins": [
        "react-intl"
      ]
    },
    "production": {
      "plugins": [
        ["react-intl", {
          "messagesDir": "lang/.messages/"
        }]
      ]
    },
    "test": {
      "presets": ["next/babel"]
    }
  },
  "plugins": [
    // [
    //   "import", {
    //     "libraryName": "antd",
    //     "style": "css"
    //   },
    // ],
    [ "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": false
      }
    ]
  ]
}
